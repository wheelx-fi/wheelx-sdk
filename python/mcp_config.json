{
  "name": "wheelx-mcp-server",
  "version": "0.1.0",
  "description": "WheelX MCP Server for DeFi swap and bridge operations",
  "dependencies": {
    "fastmcp": "^0.1.0",
    "requests": "^2.25.0"
  },
  "entry_point": "mcp_server.py",
  "protocol_version": "2024-11-05",
  "capabilities": {
    "tools": true,
    "resources": true
  },
  "supported_chains": [
    {
      "chain_id": 1,
      "name": "Ethereum Mainnet",
      "native_currency": "ETH",
      "explorer": "https://etherscan.io"
    },
    {
      "chain_id": 10,
      "name": "Optimism",
      "native_currency": "ETH",
      "explorer": "https://optimistic.etherscan.io"
    },
    {
      "chain_id": 56,
      "name": "BNB Smart Chain",
      "native_currency": "BNB",
      "explorer": "https://bscscan.com"
    },
    {
      "chain_id": 137,
      "name": "Polygon",
      "native_currency": "MATIC",
      "explorer": "https://polygonscan.com"
    },
    {
      "chain_id": 42161,
      "name": "Arbitrum One",
      "native_currency": "ETH",
      "explorer": "https://arbiscan.io"
    },
    {
      "chain_id": 8453,
      "name": "Base",
      "native_currency": "ETH",
      "explorer": "https://basescan.org"
    },
    {
      "chain_id": 43114,
      "name": "Avalanche C-Chain",
      "native_currency": "AVAX",
      "explorer": "https://snowtrace.io"
    }
  ],
  "default_settings": {
    "base_url": "https://wheelx.fi",
    "timeout": 30,
    "max_retries": 3
  }
}